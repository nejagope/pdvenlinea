{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Empresa list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nitempresa</th>
                <th>Nombreempresa</th>
                <th>Direccionempresa</th>
                <th>Mailempresa</th>
                <th>Contactoempresa</th>
                <th>Telefonoempresa</th>
                <th>Status</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>Nombrecertificado</th>
                <th>Estadosucursal</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('empresa_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nitempresa }}</td>
                <td>{{ entity.nombreempresa }}</td>
                <td>{{ entity.direccionempresa }}</td>
                <td>{{ entity.mailempresa }}</td>
                <td>{{ entity.contactoempresa }}</td>
                <td>{{ entity.telefonoempresa }}</td>
                <td>{{ entity.status }}</td>
                <td>{% if entity.createdat %}{{ entity.createdat|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.updatedat %}{{ entity.updatedat|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.nombreCertificado }}</td>
                <td>{{ entity.estadoSucursal }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('empresa_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('empresa_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('empresa_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
